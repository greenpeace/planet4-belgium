{
	"name": "greenpeace/planet4-belgium",
	"description": "Greenpeace P4 Belgium",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-child-theme" : "0.*",
		"greenpeace/planet4-plugin-migrated-urls": "0.0.1"
	},
	"scripts": {
		"core:language": "wp language core install nl_BE;",
		"core:language2": "wp language core install fr_BE;",
		"install:plugin-wpml": "wp plugin install --activate https://storage.googleapis.com/planet4-3rdparty-plugins/sitepress-multilingual-cms.4.3.4.zip",
		"install:plugin-wpml-management": "wp plugin install --activate https://storage.googleapis.com/planet4-3rdparty-plugins/wpml-translation-management.2.9.2.zip",
		"install:plugin-wpml-stringtranslate": "wp plugin install --activate https://storage.googleapis.com/planet4-3rdparty-plugins/wpml-string-translation.3.0.4.zip",
		"site:custom": [
			"@core:language",
			"@core:language2",
			"@install:plugin-wpml",
			"@install:plugin-wpml-management",
			"@install:plugin-wpml-stringtranslate"
		]
	}
}
